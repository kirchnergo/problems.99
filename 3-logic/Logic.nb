   RowBox[{
    RowBox[{"{", 
     RowBox[{"#1", ",", "#2", ",", 
      RowBox[{"(", 
       RowBox[{"#1", " ", "&&", " ", 
        RowBox[{"(", 
         RowBox[{"#1", " ", "||", " ", "#2"}], ")"}]}], ")"}]}], "}"}], "&"}],
    "@@@", 
   RowBox[{"Tuples", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"True", ",", "False"}], "}"}], ",", "2"}], "]"}]}], "//", 
  "TableForm"}]], "Input",
 CellChangeTimes->{{3.701941046300927*^9, 3.7019410484686937`*^9}, {
  3.701941262311491*^9, 3.701941416957456*^9}}],

